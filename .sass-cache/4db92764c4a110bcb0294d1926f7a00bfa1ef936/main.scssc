.portfolioNav {
  width: 100%;
  margin: 0 auto 16px;
  display: table !important;
  text-align: center;
  h2 {
    display: inline-block;
    padding: 4px 0 0;
    // text-align: center;
  }
  .prev, .next {
    background: url('/images/arrows-retina.png') 0 0 no-repeat;
    background-size: 80px 80px;
    height: 40px;
    width: 40px;
    a {
      display: block;
      height: 100%;
      width: 100%;
    }
  }
  .prev {
    float: left;
    &:hover, &:active {
      background-position: 0 -40px;
    }
  }
  .next {
    float: right;
    background-position: -40px 0;
    &:hover, &:active {
      background-position: -40px -40px;
    }
  }
}
.portfolioNav .prev { text-align: left; }
.portfolioNav .next { text-align: right; }
.portfolioNav .miniNav { text-align: center; }

/***FOOTER***/

.contact {
  color: white;
  width: 100%;
  clear: both;
  font-size: 1.2em;
  display: block;
  padding-bottom: 60px;
  margin-top: 30px;
  height: 100%;
  .contentContainer {
    padding: 8px 0;
    text-align: right;
    a {
      display: inline-block;
      margin: 6px 15px 6px 0;
      text-transform: lowercase;
      vertical-align: middle;
      div {
        display: block;
        // text-align: center;
        // vertical-align: middle;
      }
      .socialIcon {
        background: url('/images/social-icons-retina.png') 0 0 no-repeat;
        background-size: 280px 80px;
        display: inline-block;
        height: 40px;
        width: 40px;
        // margin-right: 8px;
        &.behance {
          &:hover, &:active {
            background-position: 0 -40px;
          }
        }
        &.github {
          background-position: -40px 0;
          &:hover, &:active {
            background-position: -40px -40px;
          }
        }
        &.stackExchange {
          background-position: -80px 0;
          &:hover, &:active {
            background-position: -80px -40px;
          }
        }
        &.twitter {
          background-position: -120px 0;
          &:hover, &:active {
            background-position: -120px -40px;
          }
        }
        &.linkedIn {
          background-position: -160px 0;
          &:hover, &:active {
            background-position: -160px -40px;
          }
        }
        &.email {
          background-position: -200px 0;
          &:hover, &:active {
            background-position: -200px -40px;
          }
        }
        &.phone {
          background-position: -240px 0;
          &:hover, &:active {
            background-position: -240px -40px;
          }
        }
        
      }
    }
  }
}
