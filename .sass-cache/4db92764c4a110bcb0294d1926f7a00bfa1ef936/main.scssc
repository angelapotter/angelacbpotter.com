@import "generics";

/***HEAD***/

#head {
  height: 5em;
  overflow: hidden;
  position: relative;
  margin: 20px 0 10px;
}

/***NAV***/
.leftNavContainer {
    width: 254px;
    float: left;
    // overflow: hidden;
    position: relative;
    // position: fixed;
    // top: 107px;
    // position: fixed;  
    // width: 100%;  
    left: 0;  
    top: 0;  
    // z-index: 100;  
    border-top: 0;
    .leftNav {
      margin-right: 10px;
      padding: $mainPadding 0;      
      a {
        display: block;
        padding: 5px $mainPadding;
        margin: 0;
        cursor: pointer;
        &:hover {
          text-decoration: underline;
          // @extend .active;
        }
        &.mainTab {
          font-weight: bold;
          text-transform: lowercase;
          color: $darkestGray;
        }
        &.subTab {
          color: $darkGray;
          font-size: 0.9em;
          padding-left: 40px;
        }
        &.active {
          color: $primaryColor;
        }        
      }
      
    }
  }


#nav {
  margin: 0 0 10px;
  width: 100%;
  display: table;
}
.halfNav, .tabContainer {
	border: 0;
	margin: 6px 0 0;
	padding: 0;
  display: block;
  float: left;
}
.halfNav {
  width: 50%;
}
.tabContainer {
  margin: 0 1.4%;
  width: 47.9%;
  // @include box-shadow($standardBoxShadow);
}
#nav .tab {
  color: $lightGray;
  display: block;
  font-size: 1.3em;
  text-align: center;
  width: 100%;
  padding: 0.5em 0 0.6em; 
}
#nav .tab:hover,
.activeTab {
  background-color: white !important;
  color: $primaryColor !important;
  // @include box-shadow($standardBoxShadow);
}

/***CONTENT***/
.colorBlock.top {
  background-color: $darkGray;
  height: 400px;
  top: 0;
}
.content {
  // padding: 22px 30px;
  // @include box-shadow($standardBoxShadow);
}
.profile {
  float: left;
  margin: 0 30px 0 0;
  // height: 150px;
}
.profile.web {
  height: 400px;
  width: 230px;
}
.profile.mobile {
  display: none;
}

/* page specifics */
#resume {
  .content {
    > div {
      margin-bottom: 5px;
    }
  }
  h4 {
    margin-top: 24px;
    margin-bottom: 6px;
  }
  p {
    margin-top: 1em;
    margin-bottom: 0.1em;
  }
  ul, li {
    margin-top: 0;
    margin-bottom: 0;
  }
}

.profileContainer {
  @extend .noPadBot;
  min-height: 378px;
}

/***PORTFOLIO***/

#portfolio {
  .content { overflow: hidden; }
  #items, .portfolioSidebarContainer {
    display: inline-block;
    vertical-align: top;
    // width: 33.3333%;
  }
  
  #items {
    // width: 66.6667%;
  }
  .item {
    float: left;
    width: 33.3333%;
    margin-top: 0;
    // img {
    //   width: 252px;
    //   height: 150px;
    // }
    .itemLink {
      @extend .inlineBlock;
      @extend .borderBox;
      border: 1px solid $lightGray;
      height: 150px;
      display: block;
      width: auto;
      // width: 100%;
      // width: 252px;
      margin: 0 10px 10px 0;
      position: relative;
      
      // background-size: cover;
      // float: left;
      // @include box-shadow($standardBoxShadow);
      .itemImage {
        background: url('/portfolio/images/portfolio-thumbs.png') 0 0 no-repeat;
        background-size: 500px 2000px;
        height: 100%;
        max-width: 247px;
        // width: auto;
        margin: 0 auto;
      }
    }
    &#ssa .itemLink {
      background: url('/portfolio/images/ssa-thumb-bg.png') 50% 0 no-repeat;
      background-size: cover;
      .itemImage {
        background-position: 0 0; 
      }
    }
    &#beforeItWasCool .itemLink {
      background: white;
      .itemImage {
        background-position: 0 -200px; 
      }
    }
    &#snake .itemLink {
      background: #539574;
      .itemImage {
        background-position: 0 -400px; 
      }
    }
    &#thirdLayerSoftware .itemLink {
      background: #ccc;
      .itemImage {
        background-position: 0 -600px; 
      }
    }
    &#oldPersonApp .itemLink {
      background: url('/portfolio/images/old-person-bg.png') 50% 40% no-repeat;
      background-size: cover;
      .itemImage {
        background-position: 0 -800px; 
      }
    }
    &#myFive .itemLink {
      background: white;
      .itemImage {
        background-position: 0 -1000px; 
      }
    }
    &#olsetWebsite .itemLink {
      background: #00a0cc;
      .itemImage {
        background-position: 0 -1200px; 
      }
    }
    &#olsetAnydo .itemLink {
      background: white;
      .itemImage {
        background-position: 0 -1400px; 
      }
    }
  }

  .itemOverlay {
    // display: table;
    // vertical-align: middle;
    @include opacity(0.95);
    height: 100%;
    width: 100%;
    background-color: white;
    position: absolute;
    // left: 1px;
    // padding: 20px;
    // text-align: center;
    font-size: 1.2em;
    // text-transform: uppercase;
    // font-weight: bold;
    // color: $mediumGray;
    div {
      padding: 20px;
      display: table-cell;
      vertical-align: top;
      // font-weight: bold;
      strong {
        color: $mediumGray;
        display: block;
        padding-bottom: 8px;
      }      
    }
  }  
}

/***TWITTER FEED***/
#twitterFeed .content {
  min-height: 700px;
}
.twitter-timeline {
  width: 100%;
  // height: auto;
  // min-height: 100%;
}

/*applied to multiple*/
#portfolio h4,
.portfolioPage h4, .date {
	display: inline-block;
}
.portfolioPage {
  img {
    max-width: 100%;
  }
}

#items {
	position: relative;
	clear: both;
	width: 786px;
}
.item {
	display: block;
  margin-top: 10px;
	// width: 262px;
 //  height: 160px;
  // float: left;
	// margin: 0 auto;
  p {
    margin-top: 0;
    padding-top: 0;
    margin-bottom: 10px;
    // font-size: 1.4em;
  }
}

.date {
	color: #666;
	font-size: 0.8em;
	margin-bottom: 10px;
	margin-left: 10px;
}
.border { border:2px solid #919FAC; }

hr {
	background-color: #9CA9B4;
	border: 0;
	color: #9CA9B4;
	height: 1px;
	margin: 50px 0;
	width: 100%;
}
.portfolioNav {
  display: table;
  width: 100%;
  margin: 16px auto;
  display: none;
  > div {
    display: table-cell;
    vertical-align: middle;
  }
}
.portfolioNav .miniNav {
  div {
    display: inline-block;
  }
  a {
    @extend .inlineBlock;
    height: 30px;
    width: 60px;
    background: url('/portfolio/images/portfolio-mini-nav-thumbs.png') left top no-repeat;
    background-size: 800px 30px;
    margin: 10px 2px;
    &.biwc {
      background-position: -75px 0;
    }
    &.snake {
      background-position: -150px 0;
    }
    &.thirdLayer {
      background-position: -225px 0;
    }
    &.oldPerson {
      background-position: -300px 0;
    }
    &.myfive {
      background-position: -375px 0;
    }
    &.olsetWebsite {
      background-position: -450px 0;
    }
    &.olsetAnydo {
      background-position: -525px 0;
    }
    &.active, &:hover {
      border-bottom: 2px solid white;
      margin-bottom: 2px !important;
      padding-bottom: 6px;
    }
  }
  // img {
  //   margin: 10px 2px;
  //   height: 30px;
  //   width: 60px;
  // }   
}
// portfolioNav .miniNav a.active, .portfolioNav .miniNav a:hover {
//   border-bottom: 2px solid white;
//   margin-bottom: 2px !important;
//   padding-bottom: 6px;
// }
.portfolioNav {
  .prev, .next {
    background: url('/images/arrows-retina.png') 0 0 no-repeat;
    background-size: 80px 40px;
    height: 40px;
    width: 40px;
    a {
      display: block;
      height: 100%;
      width: 100%;
    }
  }
  .next {
    background-position: -40px 0;
  }
}
.portfolioNav .prev { text-align: left; }
.portfolioNav .next { text-align: right; }
.portfolioNav .miniNav { text-align: center; }

/***FOOTER***/

.contact {
  color: white;
  width: 100%;
  clear: both;
  font-size: 1.2em;
  display: block;
  padding-bottom: 60px;
  margin-top: 4px;
  height: 100%;
  .contentContainer {
    padding-top: 8px;
    padding-bottom: 8px;
    text-align: right;
    a {
      display: inline-block;
      margin: 6px 15px 6px 0;
      text-transform: lowercase;
      vertical-align: middle;
      div {
        display: block;
        // text-align: center;
        // vertical-align: middle;
      }
      .socialIcon {
        background: url('/images/social-icons-retina.png') left top no-repeat;
        background-size: 280px 80px;
        display: inline-block;
        height: 40px;
        width: 40px;
        // margin-right: 8px;
        &.behance {
          // &:hover, &:active {
          //   background-position: 0 -40px;
          // }
        }
        &.github {
          background-position: -40px 0;
          // &:hover, &:active {
          //   background-position: -40px -40px;
          // }
        }
        &.stackExchange {
          background-position: -80px 0;
          // &:hover, &:active {
          //   background-position: -80px -40px;
          // }
        }
        &.twitter {
          background-position: -120px 0;
          // &:hover, &:active {
          //   background-position: -120px -40px;
          // }
        }
        &.linkedIn {
          background-position: -160px 0;
          // &:hover, &:active {
          //   background-position: -160px -40px;
          // }
        }
        &.email {
          background-position: -200px 0;
        }
        &.phone {
          background-position: -240px 0;
        }
        
      }
    }
  }
}
