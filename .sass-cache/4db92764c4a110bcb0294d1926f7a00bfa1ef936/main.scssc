}
.portfolioPage {
  .content {
    h2 {
      display: inline-block;
      padding-top: 0;
      margin-bottom: 25px;
    }
    .projectInfo {
      float: right;
      text-align: right;
      color: $lightGray;
    }
  }
  img {
    max-width: 100%;
  }
}

#items {
	position: relative;
	clear: both;
	width: 786px;
